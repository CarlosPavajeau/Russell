<UserControl x:Class="View.RegisterEmployeeUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:View"
             mc:Ignorable="d" 
             d:DesignHeight="730" d:DesignWidth="1080">
    <Grid Background="White">
        <StackPanel>
            <local:RegisterEmployeeFields x:Name="RegisterEmployeesField"></local:RegisterEmployeeFields>
            <Button x:Name="RegisterEmployee" Content="Registrar emplado" Width="120" Style="{StaticResource PrimaryButton}"></Button> 
        </StackPanel>  
    </Grid>
</UserControl>
